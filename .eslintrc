{
  "extends": "standard",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-underscore-dangle": [0],
    "no-use-before-define": [0],
    "comma-dangle": [0],
    "prefer-arrow-callback": [0],
    "func-names": [0]
  }
}
